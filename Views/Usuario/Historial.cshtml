@{
    Layout = "_LayoutUsuario";
}

<div class="row mt-1">

    <div class="col-lg-9 col-12 mt-3">

        <div class="pl-3 pt-2 pb-1 shadow" style="background-color:#0abbec;">

            <h5 class="text-white font-weight-bold ">Historial rendiciones</h5>

        </div>

        <div class="shadow bg-white">

            <div class="p-2">

                <div class="table-responsive p-1 mt-3">

                    <table class="table table-striped table-bordered table-hover w-100" id="TablaHistorial">

                        <thead class="thead-dark ">

                            <tr>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Código rendición</th>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Fecha Operación</th>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Hora Operación</th>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Operacion</th>

                            </tr>

                        </thead>

                        <tbody>
                        </tbody>

                    </table>

                </div>

            </div>

        </div>


    </div>



</div>


<div class="row mt-4">

    <div class="col-lg-9 col-12 mt-3">

        <div class="pl-3 pt-2 pb-1 shadow" style="background-color:#0abbec;">

            <h5 class="text-white font-weight-bold ">Historial Fondos</h5>

        </div>

        <div class="shadow bg-white">

            <div class="p-2">

                <div class="table-responsive p-1 mt-3">

                    <table class="table table-striped table-bordered table-hover w-100" id="TablaFondos">

                        <thead class="thead-dark ">

                            <tr>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Código Fondo</th>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Fecha Operación</th>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Hora Operación</th>
                                <th scope="col" class="text-center" style="font-size: 13px; font-weight: 500;">Operacion</th>

                            </tr>

                        </thead>

                        <tbody>
                        </tbody>

                    </table>

                </div>

            </div>

        </div>


    </div>



</div>

@section scripts {

    <script src="~/js/API/Historial.js"></script>
   
    <script>
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });
    </script>


}
