
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>MANAGER</title>

    <link rel="stylesheet" href="~/css/style.css">
    <link href="~/css/bootstrap.css" rel="stylesheet">
    <link href="~/css/simple-sidebar.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">


</head>

<body style="background:rgb(240, 242, 245);">

    <div class="d-flex" id="wrapper">

        <div class="bg-white border-right" id="sidebar-wrapper">


            <div class="sidebar-heading text-center" style="max-height:75px;min-height:75px;max-width:240px;min-width:240px;">

                <div class="container  text-center mx-auto d-block ">

                    <div class="row text-center mx-auto d-block">

                        <div class="col-12 pt-1 text-center mx-auto d-block">

                            <a href="/Autorizador/home">
                                <img id="logo_sociedad" style="width:100%;height:100%;" src="" alt="">
                            </a>

                        </div>

                    </div>

                </div>


            </div>

            <div class="list-group list-group-flush pt-2">


                 <a href="/Autorizador/home" class="list-group-item list-group-item-action bg-light  bg-light border"
                    role="button" aria-expanded="false"
                   style="color: #0abbec ">
                    <img src="~/img/internet.png" height="19px" class="pr-3" alt="">Home
                </a>

                <a href="#collapseExample" class="list-group-item list-group-item-action bg-light  bg-light border"
                   data-toggle="collapse" role="button" aria-expanded="false" aria-controls="collapseExample"
                   style="color: #0abbec ">
                    <img src="~/img/prueba.png" height="19px" class="pr-3" alt="">Solicitud aprobación
                </a>

                <div class="collapse show" id="collapseExample">

                    <div id="/Autorizador/Aprobaciones" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/Aprobaciones">
                            <img src="~/img/file.png" height="16px" class="pr-2 " alt=""> Fondos por rendir
                        </a>
                    </div>

                    <div id="/Autorizador/AprobacionesRendicion" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/AprobacionesRendicion">
                            <img src="~/img/file.png" height="16px" class="pr-2 " alt=""> Rendiciones
                        </a>
                    </div>

                </div>

                <a href="#collapseExample2" class="list-group-item list-group-item-action border"
                   data-toggle="collapse" role="button" aria-expanded="false" aria-controls="collapseExample">
                    <img src="~/img/checklist.png" height="19px" class="pr-3" alt="">Ducumentos
                </a>

                <div class="collapse show" id="collapseExample2">

                    <div id="D_SolicitudFondo" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/SolicitudFondo">
                            <img src="~/img/Proyectos.png" height="16px" class="pr-2 " alt=""> Solicitud de fondos
                        </a>
                    </div>


                    <div id="D_FondosSolicitados" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/MisFondos">
                            <img src="~/img/view.png" height="16px" class="pr-2 " alt=""> Mis Fondos
                        </a>
                    </div>


                    <div id="D_SolicitudRendicion" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/SolicitudRendicion">
                            <img src="~/img/bill.png" height="16px" class="pr-2 " alt=""> Solicitud de Rendicion
                        </a>
                    </div>

                    <div id="D_RendicionesSolicitadas" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/MisRendiciones">
                            <img src="~/img/view.png" height="16px" class="pr-2 " alt=""> Mis Rendiciones
                        </a>
                    </div>


                    <div id="/Autorizador/AprobacionJefe" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/AprobacionJefe">
                            <img src="~/img/file.png" height="16px" class="pr-2 " alt="">Solicitud fondos
                        </a>
                    </div>

                    <div id="/Autorizador/AprobacionJefeRendicion" class="container pt-2 border-bottom pb-2">
                        <a style="text-decoration: none; font-size: 15px;"
                           class=" text-dark ml-3   menu-opcion" href="/Autorizador/AprobacionJefeRendicion">
                            <img src="~/img/file.png" height="16px" class="pr-2 " alt="">Solicitud rendición
                        </a>

                    </div>

                </div>

                <div>
                    <a href="/Autorizador/Historial" class="list-group-item list-group-item-action  bg-white ">
                        <img src="~/img/reloj.png"
                             class="pr-3" height="19px" alt="">Historial
                    </a>
                </div>

                <div>
                    <a  href="/Autorizador/Perfil"  class="list-group-item list-group-item-action  bg-white  ">
                        <img src="~/img/usuario.png"
                             class="pr-3" height="19px" alt="">Mi perfil
                    </a>
                </div>

                <div class=" mt-3 p-2 border text-center" style="background:#fdfdfd;">
                    <span class="text-dark ">
                        Tipo usuario : <span class=" ml-3   "
                                             style="color:#0abbec ; font-weight: 600;">Autorizador</span>
                    </span>
                </div>


                <div class=" mt-3 p-2 border text-center" style="background:#fdfdfd;">
                    <span class="text-dark ">
                        Sociedad Actual :
                        <span class="text-center" style="color:#0abbec ; font-weight: 700;" id="nombreSociedad"></span>

                    </span>
                </div>

                <div class=" mt-3 p-2 border text-center" style="background:#fdfdfd;">
                    <span class="text-dark ">
                        <span class="text-center" style="color:#0abbec ; font-weight: 700;" id="nombres"></span>

                    </span>
                </div>


                <button type="submit" onclick="Cerrar()" style="background:#00b1e4; border-color:#0abbec;"
                        class="btn btn-primary mt-3 ml-2 mr-2  btn-sm shadow font-weight-bold rounded-0 ">
                    Cerrar sesion
                </button>

                <button type="submit" border-color:#0abbec;"
                        class="btn btn-secondary mt-3 ml-2 mr-2  btn-sm shadow font-weight-bold rounded-0 ">
                    <a style="text-decoration:none;" class="text-white font-weight-bold" href="/login/dashboard">Cambiar sociedad</a>
                </button>

            </div>

        </div>

        <div id="page-content-wrapper">

            <nav class="navbar navbar-expand-lg navbar-light bg-white border-bottom container-fluid ">


                <img src="~/img/menu-abierto.png" class="menu-btn" id="menu-toggle" height="24px" alt="">

                <button class="navbar-toggler" type="button" data-toggle="collapse"
                        data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                        aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto mt-1 mt-lg-0">

                      

                    </ul>
                </div>
            </nav>

            <div class="container-fluid   ">


                <main role="main" class="pb-3">
                    @RenderBody()
                </main>


            </div>
        </div>


    </div>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
    <script src="~/js/all.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/API/Autorizador/AutorizadorHome.js"></script>
</body>



@RenderSection("Scripts", required: false)

</html>