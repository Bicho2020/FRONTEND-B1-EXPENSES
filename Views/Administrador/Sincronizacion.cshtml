@{
    Layout = "_LayoutAdministrador";
}

<div class="row mt-3">

    <div class="col-8">

        <div class=" pl-3 pt-2 pb-1 shadow " style="background-color:#0abbec;">

            <h5 class="text-white font-weight-bold">Sincronizacion SAP - B1 EXPENSES</h5>

        </div>

        <div class="shadow bg-white">

            <div class="col-12 float-left p-3">

                <span >
                    Sincronización con sap <span class=" ml-3 text-danger font-weight-bold">*Campos obligatorios</span>
                </span>

            </div>

            <div class="row p-3 pt-0">

                <div class=" col-lg-12 col-12">

                    <form action="">

                        <div class="col-lg-4 col-12">

                            <div class="form-group">
                                <label for="Is_CodUsuario">Usuario</label>
                                <input disabled type="text" class="form-control rounded-0 form-control-sm mt-1"
                                       id="Is_CodUsuario" value="Vicente Muñoz">

                            </div>

                            <div class="form-group pt-2">
                                <label for="Is_Sociedad">Sociedad</label>
                                <input disabled type="text" class="form-control rounded-0 form-control-sm mt-1 "
                                       value="COD001" id="Is_Sociedad" placeholder="">
                            </div>
                        </div>

                        <div>

                            <div class="form-group pt-2">
                                <label for="Is_WebService">Web Service</label>
                                <input type="text" class="form-control rounded-0 form-control-sm mt-1"
                                       id="Is_WebService" placeholder="Ingrese web service">
                            </div>

                            <div class="form-group pt-2">
                                <label for="Is_Contrasenia">Password</label>
                                <input type="password" class="form-control rounded-0 form-control-sm mt-1"
                                       id="Is_Contrasenia" placeholder="Ingrese contraseña">
                            </div>

                        </div>

                    </form>

                </div>

            </div>

            <div class="row  p-3">

                <div class="col-12 text-center">

                    <table class="table table-striped  table-bordered">

                        <tbody>
                            <tr>

                                <td>Empleados</td>
                                <td>
                                    <div class="row">

                                        <div class="col-6">
                                            <input type="radio" id="EMPSAP" name="customRadio" class="custom-control-input">
                                            <label class="custom-control-label" for="EMPSAP">Sap</label>
                                        </div>

                                        <div class="col-6">
                                            <input type="radio" id="EMPB1" name="customRadio" class="custom-control-input">
                                            <label class="custom-control-label" for="EMPB1">B1 EXPENSES</label>
                                        </div>

                                    </div>
                                    
                                </td>
                                <td>Mensaje</td>
                            </tr>
                            <tr>

                                <td>Plan de cuentas</td>
                                <td><div class="row">

                                     <div class="col-6">
                                            <input type="radio" id="PDCSAP" name="customRadio2" class="custom-control-input">
                                            <label class="custom-control-label" for="PDCSAP">Sap</label>
                                        </div>

                                        <div class="col-6">
                                            <input type="radio" id="PDB1" name="customRadio2" class="custom-control-input">
                                            <label class="custom-control-label" for="PDB1">B1 EXPENSES</label>
                                        </div>


                                  </div>
                                </td>
                                <td>Mensaje </td>
                            </tr>
                            <tr>

                                <td>Proyectos</td>
                                <td>
                                    <div class="row">

                                        <div class="col-6">
                                            <input type="radio" id="PROSAP" name="customRadio3" class="custom-control-input">
                                            <label class="custom-control-label" for="PROSAP">Sap</label>
                                        </div>

                                        <div class="col-6">
                                            <input type="radio" id="PROB1" name="customRadio3" class="custom-control-input">
                                            <label class="custom-control-label" for="PROB1">B1 EXPENSES</label>
                                        </div>

                                    </div>
                                </td>
                                <td>Mensaje</td>
                            </tr>
                            <tr>

                                <td>Centro de costos</td>
                                <td>
                                    <div class="row">

                                        <div class="col-6">
                                            <input type="radio" id="customRadio1" name="customRadio4" class="custom-control-input">
                                            <label class="custom-control-label" for="customRadio1">Sap</label>
                                        </div>

                                        <div class="col-6">
                                            <input type="radio" id="customRadio2" name="customRadio4" class="custom-control-input">
                                            <label class="custom-control-label" for="customRadio2">B1 EXPENSES</label>
                                        </div>

                                    </div>
                                </td>
                                <td>Mensaje</td>
                            </tr>

                            <tr>

                                <td>Articulos</td>
                                <td>
                                    <div class="row">

                                        <div class="col-6">
                                            <input type="radio" id="CCSAP" name="customRadio5" class="custom-control-input">
                                            <label class="custom-control-label" for="CCSAP">Sap</label>
                                        </div>

                                        <div class="col-6">
                                            <input type="radio" id="CCB1" name="customRadio5" class="custom-control-input">
                                            <label class="custom-control-label" for="CCB1">B1 EXPENSES</label>
                                        </div>

                                    </div>
                                </td>
                                <td>Mensaje</td>
                            </tr>
                        </tbody>
                    </table>

                </div>

            </div>


        </div>

    </div>

</div>



<script>
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });
</script>

